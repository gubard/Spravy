<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="https://github.com/projektanker/icons.avalonia">
    <Style Selector="i|Icon">
        <Setter Property="Foreground" Value="{DynamicResource SystemControlForegroundBaseHighBrush}" />
        <Setter Property="FontSize" Value="16.8" />
    </Style>

    <Style Selector="i|Icon.h1">
        <Setter Property="FontSize" Value="36.4" />
    </Style>

    <Style Selector="i|Icon.h2">
        <Setter Property="FontSize" Value="28" />
    </Style>

    <Style Selector="i|Icon.h3">
        <Setter Property="FontSize" Value="22.4" />
    </Style>

    <Style Selector="i|Icon.h4">
        <Setter Property="FontSize" Value="19.6" />
    </Style>

    <Style Selector="i|Icon.h5">
        <Setter Property="FontSize" Value="16.8" />
    </Style>

    <Style Selector="i|Icon.h6">
        <Setter Property="FontSize" Value="14" />
    </Style>

    <Style Selector="i|Icon.h6">
        <Setter Property="FontSize" Value="11.2" />
    </Style>
</Styles>