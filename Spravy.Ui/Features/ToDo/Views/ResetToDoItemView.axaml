<ru:ReactiveUserControl x:TypeArguments="vm:ResetToDoItemViewModel"
                        xmlns="https://github.com/avaloniaui"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                        xmlns:ru="http://reactiveui.net"
                        xmlns:vm="clr-namespace:Spravy.Ui.Features.ToDo.ViewModels"
                        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                        x:Class="Spravy.Ui.Features.ToDo.Views.ResetToDoItemView"
                        x:DataType="vm:ResetToDoItemViewModel">
    <StackPanel>
        <CheckBox IsChecked="{Binding IsCompleteTask}" Content="{DynamicResource ResetToDoItemView.CompleteTask}" />
        <CheckBox IsChecked="{Binding IsMoveCircleOrderIndex}"
                  Content="{DynamicResource ResetToDoItemView.MoveCircleOrderIndex}" />
    </StackPanel>
</ru:ReactiveUserControl>