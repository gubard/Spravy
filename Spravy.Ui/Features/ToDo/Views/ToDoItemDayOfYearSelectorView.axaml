<ru:ReactiveUserControl x:TypeArguments="viewModels:ToDoItemDayOfYearSelectorViewModel"
                        xmlns="https://github.com/avaloniaui"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                        xmlns:ru="http://reactiveui.net"
                        xmlns:m="clr-namespace:Spravy.Ui.Models"
                        xmlns:viewModels="clr-namespace:Spravy.Ui.Features.ToDo.ViewModels"
                        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                        x:Class="Spravy.Ui.Features.ToDo.Views.ToDoItemDayOfYearSelectorView"
                        x:DataType="viewModels:ToDoItemDayOfYearSelectorViewModel">
    <Interaction.Behaviors>
        <EventTriggerBehavior EventName="Initialized">
            <InvokeCommandAction Command="{Binding InitializedCommand}" />
        </EventTriggerBehavior>
    </Interaction.Behaviors>

    <TabControl ItemsSource="{Binding Items}">
        <TabControl.ItemTemplate>
            <DataTemplate x:DataType="m:DayOfYearSelectItem">
                <TextBlock Text="{Binding Month}" />
            </DataTemplate>
        </TabControl.ItemTemplate>
    </TabControl>
</ru:ReactiveUserControl>