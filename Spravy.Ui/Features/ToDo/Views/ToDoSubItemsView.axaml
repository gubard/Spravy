<ru:ReactiveUserControl x:TypeArguments="viewModels:ToDoSubItemsViewModel"
                        xmlns="https://github.com/avaloniaui"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                        xmlns:ru="http://reactiveui.net"
                        xmlns:c="clr-namespace:SukiUI.Controls;assembly=SukiUI"
                        xmlns:viewModels="clr-namespace:Spravy.Ui.Features.ToDo.ViewModels"
                        xmlns:views="clr-namespace:Spravy.Ui.Features.ToDo.Views"
                        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                        x:Class="Spravy.Ui.Features.ToDo.Views.ToDoSubItemsView"
                        x:DataType="viewModels:ToDoSubItemsViewModel">
    <ContentControl Name="{x:Static views:ToDoSubItemsView.ListContentControlName}" Content="{Binding List}">
        <ContentControl.Styles>
            <Style Selector="Expander">
                <Setter Property="VerticalAlignment" Value="Top" />
            </Style>
            <Style Selector="c|GlassCard">
                <Setter Property="Margin" Value="0,0,0,5" />
            </Style>
        </ContentControl.Styles>
    </ContentControl>
</ru:ReactiveUserControl>